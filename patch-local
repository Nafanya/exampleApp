sdb root on

#clear error files
sdb shell rm -rf /opt/share/crash/dump/*
sdb shell rm -rf /opt/share/crash/temp/*

#create repository for patch files
sdb shell mkdir -p /tmp/patchtmp

#temporary : Set VCONF Value for Battery
sdb shell vconftool set -t int -i memory/sysman/battery_status_low 5
sdb shell vconftool set -t int -i memory/sysman/battery_level_status 4
#lcd timeout set
sdb shell vconftool set -t int db/setting/lcd_backlight_normal 1000000

#enable chromium-efl.mini_browser in menu-screen
sdb shell sed '6s/nodisplay=\"true\"/nodisplay=\"false\"/' /usr/share/packages/org.tizen.chromium-efl.xml > org.tizen.chromium-efl.xml
sdb push org.tizen.chromium-efl.xml /usr/share/packages/org.tizen.chromium-efl.xml
rm -rf org.tizen.chromium-efl.xml
sdb shell tpk-backend -y org.tizen.chromium-efl --preload

#temporary : copy necessary files
sdb push local/*.mp4 /home/owner/

#for mmfw for audio and video
sdb shell rpm -e mmfw-sysconf-target-artik10
sdb push local/mmfw-sysconf-target-artik10*.rpm /tmp/patchtmp/
sdb shell rpm -Uvh --force --nodeps /tmp/patchtmp/mmfw-sysconf-target-artik10*.rpm

#model config temporary
sdb shell rpm -e --nodeps model-config-generic
sdb push local/model-config-artik*.rpm /tmp/patchtmp
sdb shell rpm -Uvh --force --nodeps /tmp/patchtmp/model-config-artik*.rpm

sdb shell sync
sdb shell sync
sdb shell sync

sdb shell reboot -f
